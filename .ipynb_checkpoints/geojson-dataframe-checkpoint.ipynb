   "source": [
    "import pandas as pd\n",
    "import requests\n",
    "import json\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "8ca95940-6d51-4e09-830f-c27199e43899",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Fetch GeoJSON data from URL\n",
    "url = 'https://raw.githubusercontent.com/missinglink/uk-postcode-polygons/master/geojson/G.geojson'\n",
    "response = requests.get(url)\n",
    "data = response.json()\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "bafe2b48-6b4c-4770-a837-9119dc8d8384",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Ignoring feature with unsupported geometry type: GeometryCollection\n"
     ]
    }
   ],
   "source": [
    "# Flatten GeoJSON\n",
    "flat_data = []\n",
    "for feature in data['features']:\n",
    "    properties = feature['properties']\n",
    "    geometry = feature.get('geometry', None)  # Check if 'geometry' key exists\n",
    "    if geometry:\n",
    "        geometry_type = geometry.get('type', None)\n",
    "        if geometry_type == 'Polygon':\n",
    "            coordinates = geometry.get('coordinates', None)\n",
    "            if coordinates:\n",
    "                row = {'geometry_type': geometry_type, 'geometry_coordinates': coordinates}\n",
    "                row.update(properties)\n",
    "                flat_data.append(row)\n",
    "        else:\n",
    "            print(f\"Ignoring feature with unsupported geometry type: {geometry_type}\")\n",
    "    else:\n",
    "        print(\"Ignoring feature with missing geometry\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "07b4417f-8627-433e-894e-353ca902b382",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Create DataFrame\n",
    "df = pd.DataFrame(flat_data)\n",
    "\n"
   ]
  },
